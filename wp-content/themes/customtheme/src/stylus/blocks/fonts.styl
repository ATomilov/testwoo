@font-face {
    font-family: 'MintTypeRistrettoSlabProRegular';
    src: url('assets/fonts/MintTypeRistrettoSlabProRegular.eot');
    src: url('assets/fonts/MintTypeRistrettoSlabProRegular.eot') format('embedded-opentype'),
         url('assets/fonts/MintTypeRistrettoSlabProRegular.woff2') format('woff2'),
         url('assets/fonts/MintTypeRistrettoSlabProRegular.woff') format('woff'),
         url('assets/fonts/MintTypeRistrettoSlabProRegular.ttf') format('truetype'),
         url('assets/fonts/MintTypeRistrettoSlabProRegular.svg#MintTypeRistrettoSlabProRegular') format('svg');
}

